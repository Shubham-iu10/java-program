// Print the prime number list

package basicProgram;

public class PrimeNowithboolean {

	public static boolean IsPrimeNumber(int num) {
		
		if (num<=1){
		return false;
		}
		
		for(int i=2; i<num; i++) {
			if (num % i == 0) {
				return false;
			}
		}
		return true;
	}
	
	public static void getPrimeNumber(int num) {
		for (int i=2; i<= num; i++) {
			if(IsPrimeNumber(i));
			System.out.print(i + " ");
		}
	}
		
		public static void main(String[] arg) {			
			getPrimeNumber(7);
		}
	}
