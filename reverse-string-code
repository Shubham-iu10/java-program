// Reverse String using With for loop.

package basicProgram;

public class ReverseString {
	
	public static void main(String[]arg) 
	
	{
		String s= "Singsys";
		
		int len= s.length();
		String rev= "";
		
		for(int i=len-1; i>=0; i--);{
		
			rev = rev+s.charAt(i);	
		}
		
		System.out.println(rev);
	}

}


